          example: '8805'
        - name: Accept
          in: header
          schema:
            type: string
          example: application/json
        - name: startDate
          in: query
          schema:
            type: string
          description: date format YYYY-MM-DD
          example: string
        - name: endDate
          in: query
          schema:
            type: string
          description: date format YYYY-MM-DD
          example: string
        - name: page
          in: query
          schema:
            type: number
          description: page number, default 1
          example: '3668.277565660176'
        - name: size
          in: query
          schema:
            type: number
          description: page size default 100
          example: '96.93080000552878'
        - name: order
          in: query
          schema:
            type: string
          description: '''asc'' or ''desc'', default ''asc'''
          example: asc
      responses:
        '200':
          description: OK
          headers:
            Content-Type:
              schema:
                type: string
                example: application/json
          content:
            application/json:
              schema:
                type: object
              example:
                status: true
                data:
                  - snapshot_time: '12:48:00'
                    snapshot_time_zone: UTC+05:30
                    total: 0.181
                    snapshot_date: '2022-03-24'
                    fmv_market_code: $__USD
