openapi: 3.0.0
info:
  title: Cryptoworth Portfolio Private API
  description: >-
    Cryptoworth's Private Portfolio API. You can use this API to query data from
    your portfolio. Please see the help center articles
    (https://help.cryptoworth.app)  for more support or contact using the link
    below for additional support. This API can only be used by registered
    business users. <br/> <br/> Download the postman collection
    https://api.cryptoworth.app/docs/Cryptoworth%20Private%20API.postman_collection.json


    Contact Support:
     Email: support@cryptoworth.app
  version: 1.0.0
servers:
  - url: http://{{baseurl}}
tags:
  - name: Transactions
  - name: Connections
  - name: Snapshots
  - name: Classifications
  - name: DeFi
  - name: NFTs
  - name: Labels
  - name: Calculations
  - name: Invoices
  - name: Bills
paths:
  /transactions:
    get:
      tags:
        - Transactions
      summary: List Transactions
      description: |
        Get all portfolio transactions
      parameters:
        - name: X-API-KEY
          in: header
          schema:
            type: string
          description: Pass your API KEY here
          example: 07ed01fbe0ab4b77bacad9d704d72f2c
        - name: X-PORTFOLIO-ID
          in: header
          schema:
            type: integer
          description: Pass your portfolio ID
          example: '4423'
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
        - name: Accept
          in: header
          schema:
            type: string
          example: application/json
        - name: offset
          in: query
          schema:
            type: integer
          description: pagination offset
          example: '8805'
        - name: size
          in: query
          schema:
            type: integer
          description: number of records per call
          example: '5560'
        - name: startDate
          in: query
          schema:
            type: integer
          description: >-
            pass a start date in unix millisecond format to query transactions
            equal or after this date
          example: '8805'
        - name: endDate
          in: query
          schema:
            type: integer
          description: >-
            pass a start date in unix millisecond format to query transactions
            equal or before this date
          example: '8805'
      responses:
        '200':
          description: OK
          headers:
            Content-Type:
              schema:
                type: string
                example: application/json
          content:
            application/json:
              schema:
                type: object
              example:
                status: true
                data:
                  - cost: 107.2403
                    created_at_unix: 1510872881000
                    crypto_id: 4217
                    desc: Final gatehub transfer
                    from_dest: rPujGTiwMnKmMbAiUT6UjpFxT9ArDn1kJP
                    from_dest_name: Binance (XRP) Wallet
                    id: 1
                    in_amount: 363.526411
                    in_crypto: XRP
                    label: gatehub
                    out_amount: 363.526411
                    out_crypto: XRP
                    sale_price: 170.2403
                    sync_identifier: XRP Wallet
                    sync_source: public address
                    sync_source_name: Binance
                    tag_type: deposit
                    to_dest: rwawh2ft7uNopY93rCqykubebnWAn3VJY3
                    to_dest_name: Gatehub Wallet
                    transaction_fee: 0.000015
                    transaction_fee_type: string
                    transaction_type: send
                    tx_id: >-
                      AB3EC5C924EB1284F87BEA15E5C05077941C6C6ENN808072K526EDC7EC94745C
                  - cost: 107.2403
                    created_at_unix: 1510872881000
                    crypto_id: 8551
                    desc: Final gatehub transfer
                    from_dest: rPujGTiwMnKmMbAiUT6UjpFxT9ArDn1kJP
                    from_dest_name: Binance (XRP) Wallet
                    id: 1
                    in_amount: 363.526411
                    in_crypto: XRP
                    label: gatehub
                    out_amount: 363.526411
                    out_crypto: XRP
                    sale_price: 170.2403
                    sync_identifier: XRP Wallet
                    sync_source: public address
                    sync_source_name: Binance
                    tag_type: deposit
                    to_dest: rwawh2ft7uNopY93rCqykubebnWAn3VJY3
                    to_dest_name: Gatehub Wallet
                    transaction_fee: 0.000015
                    transaction_fee_type: string
                    transaction_type: send
                    tx_id: >-
                      AB3EC5C924EB1284F87BEA15E5C05077941C6C6ENN808072K526EDC7EC94745C
        '401':
          description: Unauthorized
          content:
            text/plain:
              schema:
                type: string
              example: ''
  /transaction_meta:
    get:
      tags:
        - Transactions
      summary: Read Transaction
      description: |
        Get vanilla information about the transaction 
      parameters:
        - name: X-API-KEY
          in: header
          schema:
            type: string
          description: Pass your API KEY here
          example: string
        - name: X-PORTFOLIO-ID
          in: header
          schema:
            type: integer
          description: Pass your portfolio ID
          example: '8805'
        - name: Accept
          in: header
          schema:
            type: string
          example: application/json
        - name: transactionId
          in: query
          schema:
            type: integer
          description: internal transaction ledger id
          example: '8805'
      responses:
        '200':
          description: OK
          headers:
            Content-Type:
              schema:
                type: string
                example: application/json
          content:
            application/json:
              schema:
                type: object
              example:
                status: true
                data:
                  - blockNumber: '0000'
                    timeStamp: '0000'
                    hash: '0x0000000'
                    nonce: xrp.png
                    blockHash: '0x0000000'
                    from: '0x00000'
                    to: '0x00000'
                  - blockNumber: '0000'
                    timeStamp: '0000'
                    hash: '0x0000000'
                    nonce: xrp.png
                    blockHash: '0x0000000'
                    from: '0x00000'
                    to: '0x00000'
        '401':
          description: Unauthorized
          content:
            text/plain:
              schema:
                type: string
              example: ''
